{
  "name": "@birchill/bugsnag-zero",
  "version": "0.4.14",
  "description": "A lightweight Bugsnag client",
  "repository": "https://github.com/birchill/bugsnag-zero.git",
  "author": "Birchill, Inc.",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "pinst --disable && rollup -c",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "release": "standard-version"
  },
  "bugs": {
    "url": "https://github.com/birchill/bugsnag-zero/issues"
  },
  "files": [
    "dist/**",
    "lambda-context.*"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "homepage": "https://github.com/birchill/bugsnag-zero#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "8.3.1",
    "@types/aws-lambda": "8.10.92",
    "@types/node": "17.0.18",
    "@types/ua-parser-js": "0.7.36",
    "husky": "7.0.4",
    "lint-staged": "12.3.4",
    "pinst": "3.0.0",
    "prettier": "2.5.1",
    "rollup": "2.67.2",
    "standard-version": "9.3.2",
    "tslib": "2.3.1",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "aws-lambda": "^1.0.6",
    "ua-parser-js": "^1.0.2"
  }
}
