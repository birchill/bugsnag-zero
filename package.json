{
  "name": "@birchill/bugsnag-zero",
  "version": "0.4.13",
  "description": "A lightweight Bugsnag client",
  "repository": "https://github.com/birchill/bugsnag-zero.git",
  "author": "Birchill, Inc.",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "pinst --disable && rollup -c",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "release": "standard-version"
  },
  "bugs": {
    "url": "https://github.com/birchill/bugsnag-zero/issues"
  },
  "files": [
    "dist/**",
    "lambda-context.*"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "homepage": "https://github.com/birchill/bugsnag-zero#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/aws-lambda": "^8.10.83",
    "@types/node": "^16.9.1",
    "@types/ua-parser-js": "^0.7.36",
    "husky": "^7.0.2",
    "lint-staged": "^12.0.2",
    "pinst": "^2.1.6",
    "prettier": "^2.4.0",
    "rollup": "^2.56.3",
    "standard-version": "^9.3.1",
    "tslib": "^2.3.1",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "aws-lambda": "^1.0.6",
    "ua-parser-js": "^1.0.2"
  }
}
